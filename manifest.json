{

  "name": "Daft.ie Price per person",
  "manifest_version": 2,
  "version": "0.0.2",
  "description": "Shows price per person when browsing daft.",
    
  "permissions": [
    "*://*.daft.ie/*/houses-to-rent/*"
  ],
  
  "icons": { "16": "icon16.png", "48": "icon48.png" },
            
  "content_scripts": [
    {
      "matches": ["*://*.daft.ie/*/houses-to-rent/*"],
      "js": ["jquery.js","monitor.js"] ,
      "css": ["daft-plugin.css"]    
    }
  ]
  
}
